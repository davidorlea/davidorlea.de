{
  "name": "davidorlea.de",
  "version": "0.1.0",
  "description": "Personal Website",
  "repository": "https://github.com/davidorlea/davidorlea.de",
  "author": "David Orlea",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "prestart": "rm -rf build/dist",
    "start": "parcel serve src/index.html --cache-dir build/cache --out-dir build/dist --open",
    "prewatch": "rm -rf build/dist",
    "watch": "parcel watch src/index.html --cache-dir build/cache --out-dir build/dist",
    "prebuild": "rm -rf build/dist",
    "build": "parcel build src/index.html --cache-dir build/cache --out-dir build/dist --no-source-maps"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.1"
  }
}
